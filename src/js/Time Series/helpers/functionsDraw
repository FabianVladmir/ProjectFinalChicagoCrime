function orderObjectByMonth(obj) {
  const monthOrder = [
    'January', 'February', 'March', 'April', 'May', 'June',
    'July', 'August', 'September', 'October', 'November', 'December'
  ];

  const orderedObject = {};

  monthOrder.forEach((month, index) => {
    orderedObject[month] = obj[month];
  });

  return orderedObject;
}


export{
    orderObjectByMonth
}